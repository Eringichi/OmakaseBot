    id
    nfts (orderBy: dateMinted) {
      nftContract {
        id
        name
        symbol
      }
      tokenId
      tokenIPFSPath
      dateMinted
      isFirstSale
      nftHistory (orderBy: date, where: {event_in: ["Minted", "Listed", "PriceChanged", "Unlisted", "BuyPriceSet"], date_gt: 1656997380, date_lte: 1656997560}) {
        actorAccount {
          id
        }
        event
        date
        amountInETH
      }
      lastSalePriceInETH
    }
  }
